<% include ../partials/header.ejs %>
<h2>Posts</h2>
<div class="list-group">
    <% posts.forEach(post => { %>
        <a href="/posts/<%= post.id %>" class="list-group-item list-group-item-action">
            <h5 class="mb-1"><%= post.title %></h5>
            <p class="mb-1"><%= post.content %></p>
        </a>
    <% }) %>
</div>
<% include ../partials/footer.ejs %>
